{
    "compilerOptions": {
      "module": "CommonJS",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "target": "ESNext",
      "outDir": "./compiled",
      "allowJs": true
    },
    "include": ["*.*"],
    "exclude": ["node_modules", "compiled/**/*"],
  }